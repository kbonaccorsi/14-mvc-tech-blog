<!-- html for homepage -->
<!-- loop over each post in the posts array -->
{{#each dbPostData}}
<div class="row m-5 post justify-content-center">
    <h2 class=" heading">
        <a href="/post/2">{{dataValues.title}}</a>
    </h2>
    <p>
        {{dataValues.content}}
    </p>
    <p> - Created by {{dataValues.user.username}}</p>
</div>
{{/each}}